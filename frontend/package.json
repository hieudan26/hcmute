{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run open-browser && next dev",
    "open-browser": "start http://localhost:3000",
    "build": "next build",
    "start": "next start",
    "lint:es": "eslint . --ext ts,tsx,js",
    "lint:style": "stylelint .components/**/*.{tsx, ts} .pages/**/*.{tsx, ts} --allow-empty-input",
    "lint-test-ddt": "yarn lint:es && yarn lint:style",
    "stylelint:css": "npx stylelint .components/**/*.{tsx, ts} .pages/**/*.{tsx, ts} --allow-empty-input",
    "lint": "eslint ./components/** ./pages/** -c .eslintrc.json --ext js,jsx",
    "lint-fix": "eslint ./components/** ./pages/** -c .eslintrc.json --fix --ext js,jsx"
  },
  "dependencies": {
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache"
    ],
    "styled.ts": [
      "stylelint --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn check:type"
    }
  },
  "devDependencies": {
    "@types/node": "18.7.16",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.9",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier": "2.7.1",
    "sass": "^1.54.9",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-webpack-plugin": "^3.3.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3"
  }
}
